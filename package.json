{
  "name": "karin-plugin-meme",
  "version": "1.2.0",
  "author": "wuliya",
  "type": "module",
  "main": "lib/index.js",
  "description": "一个Karin版的表情包合成插件",
  "license": "GNU General Public License v3.0",
  "keywords": [
    "karin-plugin",
    "karin-plugin-meme"
  ],
  "homepage": "https://github.com/ClarityJS/karin-plugin-meme",
  "bugs": {
    "url": "https://github.com/ClarityJS/karin-plugin-meme/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClarityJS/karin-plugin-meme.git"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "pub": "npm publish --access public",
    "dev": "tsx watch --include \"src/**/*.ts\" src/index.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "axios-retry": "^4.5.0",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^4.0.1",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.13.5",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.21.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "neostandard": "^0.11.9",
    "node-karin": "^1.3.15",
    "pre-commit": "^1.2.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  },
  "files": [
    "config/",
    "lib/",
    "resources/",
    "LICENSE",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "karin": {
    "main": "src/index.ts",
    "apps": [
      "lib/apps"
    ],
    "ts-apps": [
      "src/apps"
    ],
    "static": [
      "resources"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ],
    "web": "./lib/web.config.js",
    "ts-web": "./src/web.config.ts"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "packageManager": "pnpm@9.13.2"
}
