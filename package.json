{
  "name": "karin-plugin-meme",
  "version": "1.1.0",
  "author": "wuliya",
  "type": "module",
  "description": "A meme plugin for Karin",
  "license": "GNU General Public License v3.0",
  "keywords": [
    "karin-plugin",
    "karin-plugin-meme"
  ],
  "homepage": "https://github.com/ClarityJS/karin-plugin-meme",
  "bugs": {
    "url": "https://github.com/ClarityJS/karin-plugin-meme/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClarityJS/karin-plugin-meme.git"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "pub": "npm publish --access public",
    "dev": "tsx watch --include \"src/**/*.ts\" src/index.ts"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios-retry": "^4.5.0",
    "form-data": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@stylistic/eslint-plugin-js": "^3.0.0",
    "@types/node": "^22.10.10",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.19.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.14.0",
    "neostandard": "^0.11.9",
    "node-karin": "^1.2.3",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0"
  },
  "files": [
    "config/",
    "lib/",
    "resources/",
    "LICENSE",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "karin": {
    "main": "src/index.ts",
    "apps": [
      "lib/apps"
    ],
    "ts-apps": [
      "src/apps"
    ],
    "static": [
      "resources"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
